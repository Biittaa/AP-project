<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo43.EnteryController">
   <opaqueInsets>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </opaqueInsets>
   <children>
      <Label alignment="CENTER" cache="true" layoutX="71.0" layoutY="322.0" prefHeight="89.0" prefWidth="459.0" text="Welcome To Twitter!" textFill="#b87639" wrapText="true">
         <font>
            <Font name="Comic Sans MS Bold" size="44.0" />
         </font>
      </Label>
      <ImageView fitHeight="247.0" fitWidth="242.0" layoutX="180.0" layoutY="47.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../pictures/symbole-messager-jaune.png" />
         </image>
      </ImageView>
      <Button layoutX="118.0" layoutY="464.0" mnemonicParsing="false" onAction="#signIn" opacity="0.86" prefHeight="82.0" prefWidth="124.0" style="-fx-background-color: #ebc967;" text="Sign in" textFill="#588643">
         <font>
            <Font name="Comic Sans MS Bold" size="26.0" />
         </font></Button>
      <Button layoutX="357.0" layoutY="464.0" mnemonicParsing="false" onAction="#signUp" opacity="0.84" prefHeight="82.0" prefWidth="130.0" style="-fx-background-color: #97da64;" text="Sign up" textFill="#8a4e2b" wrapText="true">
         <font>
            <Font name="Comic Sans MS Bold" size="25.0" />
         </font></Button>
   </children>
</AnchorPane>
