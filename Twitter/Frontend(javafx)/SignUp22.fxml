<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo43.SignUp2Controller">
   <opaqueInsets>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </opaqueInsets>
   <children>
      <Label layoutX="212.0" layoutY="-4.0" prefHeight="42.0" prefWidth="171.0" text="Sign up" textAlignment="CENTER" textFill="#447926">
         <font>
            <Font name="Comic Sans MS Bold" size="48.0" />
         </font>
      </Label>
      <Label layoutX="86.0" layoutY="120.0" text="Country" textFill="#6b8a1e">
         <font>
            <Font name="Comic Sans MS Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="209.0" text="Date of Birth" textFill="#6b8a1e">
         <font>
            <Font name="Comic Sans MS Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="country" editable="false" layoutX="241.0" layoutY="117.0" mouseTransparent="true">
         <font>
            <Font name="Comic Sans MS" size="20.0" />
         </font>
      </TextField>
      <Button layoutX="30.0" layoutY="17.0" mnemonicParsing="false" onAction="#backButton" opacity="0.66" style="-fx-background-color: #fcb64e;" text="back">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Button>
      <Label layoutX="242.0" layoutY="268.0" prefHeight="18.0" prefWidth="206.0">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Label>
      <MenuButton layoutX="419.0" layoutY="117.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="98.0" text="Countries">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#chooseIran" text="Iran" />
          <MenuItem mnemonicParsing="false" onAction="#chooseChina" text="China" />
            <MenuItem mnemonicParsing="false" onAction="#chooseBrazil" text="Brazil" />
            <MenuItem mnemonicParsing="false" onAction="#chooseGermany" text="Germany" />
            <MenuItem mnemonicParsing="false" onAction="#chooseItaly" text="Italy" />
            <MenuItem mnemonicParsing="false" onAction="#chooseEngland" text="England" />
            <MenuItem mnemonicParsing="false" onAction="#chooseUS" text="US" />
            <MenuItem mnemonicParsing="false" onAction="#chooseNone" text="None" />
        </items>
         <font>
            <Font name="Comic Sans MS Italic" size="13.0" />
         </font>
      </MenuButton>
      <DatePicker fx:id="bornDate" layoutX="241.0" layoutY="206.0" prefHeight="42.0" prefWidth="274.0" promptText="Month/Day/Year" />
      <Button layoutX="488.0" layoutY="534.0" mnemonicParsing="false" onAction="#nextButton" opacity="0.98" prefHeight="52.0" prefWidth="98.0" style="-fx-background-color: #fcb64e;" text="Next" textFill="#3c6806">
         <font>
            <Font name="Comic Sans MS Bold" size="25.0" />
         </font>
      </Button>
      <Label fx:id="emptyBornDate" layoutX="242.0" layoutY="248.0" prefHeight="23.0" prefWidth="243.0" textFill="#f54216">
         <font>
            <Font name="Comic Sans MS" size="14.0" />
         </font>
      </Label>
      <Label fx:id="emptyCountry" layoutX="242.0" layoutY="159.0" prefHeight="23.0" prefWidth="243.0" textFill="#f54216">
         <font>
            <Font name="Comic Sans MS" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
